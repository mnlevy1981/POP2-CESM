# check to see if preformed_tracers is present before appending it
# check for an exact module name match, to avoid false matches
array_of_tracer_modules=(`./xmlquery OCN_TRACER_MODULES --value`)
found=0
for mod in ${array_of_tracer_modules[@]}; do
  if [[ $mod == "preformed_tracers" ]] ; then
    found=1
  fi
done
if [[ $found == 0 ]]; then
    ./xmlchange -a OCN_TRACER_MODULES=preformed_tracers
fi
